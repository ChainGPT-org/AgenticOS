{
    "buildCommand": "bun install && bun run build",
    "devCommand": "bun run dev",
    "outputDirectory": "dist",
    "builds": [
        {
          "src": "src/index.ts",
          "use": "@vercel/node"
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "src/index.ts"
        }
      ]
  }